<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>DuckDuckGo</os:ShortName>
<os:Description>DuckDuckGo Search</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>https://www.duckduckgo.com/</SearchForm>
<os:Url type="application/x-suggestions+json" method="GET" template="https://www.duckduckgo.com/?q={searchTerms}">
  <os:Param name="query" value="{searchTerms"/>
  <os:Param name="form" value="OSDJAS" />
</os:Url>
<os:Url type="text/html" method="GET" template="https://www.duckduckgo.com/">
  <os:Param name="q" value="{searchTerms}"/>
  <os:Param name="form" value="MOZSBR"/>
  <os:Param name="form" value="MOZLBR"/>
</os:Url>
</SearchPlugin>
